<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/sand_dark"
    tools:context=".fragments.DescricaoFragment">


    <View
        android:id="@+id/container_1"
        style="@style/edit_desc_produto"
        android:layout_width="363dp"
        android:layout_height="634dp"
        android:layout_marginTop="80dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bt_login"
        style="@style/Button_Produto"
        android:layout_width="156dp"
        android:layout_height="35dp"
        android:text="LOGIN"
        android:textCursorDrawable="@drawable/ic_baseline_shopping_basket_24"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/container_1"
        app:layout_constraintEnd_toEndOf="@+id/container_1"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/container_1"
        app:layout_constraintTop_toTopOf="@+id/container_1"
        app:layout_constraintVertical_bias="0.195"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bt_grupo"
        style="@style/Button_Produto"
        android:layout_width="156dp"
        android:layout_height="35dp"
        android:layout_marginTop="32dp"
        android:text="GRUPO"
        android:textCursorDrawable="@drawable/ic_baseline_shopping_basket_24"
        android:textSize="10dp"
        app:layout_constraintEnd_toEndOf="@+id/container_1"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/container_1"
        app:layout_constraintTop_toBottomOf="@+id/bt_login"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>